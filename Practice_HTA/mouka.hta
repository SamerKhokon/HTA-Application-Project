<html ln="en">
<head>
		<HTA:APPLICATION
		  ID="objHTA_Info"
		  APPLICATIONNAME="HTA_Info" 
		  BORDERSTYLE = "Normal"
		  CAPTION = "No"
		  CONTEXTMENU = "No"
		  INNERBORDER = "No"
		  MAXIMIZEBUTTON = "No"
		  MINIMIZEBUTTON = "No"
		  NAVIGABLE = "No"
		  SCROLL = "No"
		  SCROLLFLAT = "No"
		  SELECTION = "No"			 
		  SHOWINTASKBAR = "No"
		  SINGLEINSTANCE = "Yes"		
		  WINDOWSTATE="normal"
		/>
	<script Language="VBScript">
		Dim fso,oShell,strUser
		
		Set oShell = CreateObject("WScript.Shell")
        Set fso = CreateObject("Scripting.FileSystemObject")
        strComputerName = oShell.ExpandEnvironmentStrings( "%COMPUTERNAME%" )	
		FullName  = replace(objHTA_Info.commandLine,chr(34),"")  
		arrFN     = split(FullName,"\")  
		FileName  = arrFN(ubound(arrFN))
		SourceDir = replace(FullName,FileName,"") 	
		FullName  = SourceDir & FileName
		
		
		//oShell.Run "C:\Users\zahid\Desktop\Practice_HTA\fuck.bat"
	</script>
</head>
